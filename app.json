{
  "expo": {
    "name": "elevationW",
    "slug": "elevationW",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./img/elevationWLogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
        "bundler": "metro",
        "favicon": "img/elevationWLogo.png"
    }
  }
}
